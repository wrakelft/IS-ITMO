<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.example.model.Coordinates" table="coordinates">
        <id name="id" type="long" column="id">
            <generator class="native"/>
        </id>

        <property name="x" type="float" column="x" not-null="true"/>
        <property name="y" type="long" column="y" not-null="true"/>
    </class>
</hibernate-mapping>